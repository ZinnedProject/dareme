<h2>Dashboard</h2>

<h3>Notification Stream</h3>

<%# paginate @events %>
<br />
<table>
  <tr>
    <th>Message</th>
		<th>Time</th>
  </tr>

<% @notifications.each do |n| %>
  <tr> 
    <td><%= link_to n.followable.title, n.followable%> <%=n.content%></td>
    <td><%= n.created_at %></td>
  </tr>
<% end %>
</table>

